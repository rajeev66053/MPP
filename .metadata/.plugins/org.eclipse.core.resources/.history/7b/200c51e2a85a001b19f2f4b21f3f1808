public class Address {
	private final String id;
	
	private String street;
	
	private String city;
	
	private String state;
	
	private String zip;
	
	public Address(String street, String state, String city, String zip) {
		this.id = UUID.randomUUID().toString();
		this.street = street;
		this.state = state;
		this.city = city;
		this.zip = zip;
	}

