package system;

import java.util.UUID;

public class Agent {
	private String id;
	private String name;
	
	public Agent(String name) {
		super();
		this.id = UUID.randomUUID().toString();
		this.name = name;
	}
	
	public String getId() {
		return id;
	}
	
	public String getName() {
		return name;
	}

	public void setname(String name) {
		this.name = name;
	}
	
	@Override
   public String toString() {
        return "Address [id: " + this.id+", name: " + this.name+"]";
   }


}
